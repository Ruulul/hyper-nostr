#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dns-query@0.11.2/node_modules/dns-query/bin/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dns-query@0.11.2/node_modules/dns-query/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dns-query@0.11.2/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dns-query@0.11.2/node_modules/dns-query/bin/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dns-query@0.11.2/node_modules/dns-query/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dns-query@0.11.2/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../dns-query/bin/dns-query" "$@"
else
  exec node  "$basedir/../dns-query/bin/dns-query" "$@"
fi
