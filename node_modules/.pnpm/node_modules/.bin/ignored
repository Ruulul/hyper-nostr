#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dotignore@0.1.2/node_modules/dotignore/bin/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dotignore@0.1.2/node_modules/dotignore/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dotignore@0.1.2/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dotignore@0.1.2/node_modules/dotignore/bin/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dotignore@0.1.2/node_modules/dotignore/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/dotignore@0.1.2/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-lfs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../dotignore/bin/ignored" "$@"
else
  exec node  "$basedir/../dotignore/bin/ignored" "$@"
fi
